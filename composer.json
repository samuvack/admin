{
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/VandorpeDavid/silex-postgres-simpleuser"
        }
    ],
    "require": {
        "silex/silex": "~1.2",
        "twig/twig": "~1.18",
        "symfony/twig-bridge": "~2.6",
		"symfony/form":"~2.3",
		"symfony/security-csrf":"~2.4",
		"symfony/validator":"~2.3",
		"symfony/config":"~2.3",
		"symfony/translation":"~2.3",
        "symfony/finder": "~2.7",
        "dflydev/doctrine-orm-service-provider": "1.0.*",
        "knplabs/console-service-provider": "^1.0",
        "robmorgan/phinx": "^0.4.4",
        "davdorpe/silex-postgres-simpleuser": "dev-bleeding-edge"
    },
    
    "autoload": {
        "psr-4": {
            "MyApp\\Entities\\": "src/entities",
            "MyApp\\Converters\\": "src/converters",
            "MyApp\\Entities\\Repositories\\": "src/entities/repositories",
            "MyApp\\Database\\Functions\\": "src/database/functions",
            "MyApp\\Database\\Types\\": "src/database/types"
        }
    }
}
