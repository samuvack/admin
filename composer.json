    "require": {
        "davdorpe/silex-postgres-simpleuser": "dev-bleeding-edge",
        "dflydev/doctrine-orm-service-provider": "1.0.*",
        "knplabs/console-service-provider": "^1.0",
        "php":  ">=5.5",
        "robmorgan/phinx": "^0.4.4",
        "twig/twig": "~1.18",
